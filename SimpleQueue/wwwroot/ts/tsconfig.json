{
  "compilerOptions": {
    "types": [ "node" ],
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "module": "es6",
    "moduleResolution": "Node",
    "target": "es5",
    "outDir": "../js"
  },
  "compileOnSave": true,
  "include": [
    "*.*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}